{% extends "layout.twig" %}

{% block content %}
  <div class="page-header">
    <h3>Subscriptions</h3>
  </div>
  <table class="table">
    <thead>
      <th>#</th>
      <th>Customer #</th>
      <th>Offer #</th>
      <th>Date Sub</th>
      <th>Date Unsub</th>
      <th>Date Eff Unsub</th>
    </thead>
    {% for subscription in subscriptions %}
    <tr>
      <td><a href="#">{{subscription.id}}</a></td>
      <td>{{subscription.customer_id}}</td>
      <td>{{subscription.offer_id}}</td>
      <td>{{subscription.date_sub}}</td>
      <td>{{subscription.date_unsub}}</td>
      <td>{{subscription.date_eff_unsub}}</td>
    </tr>
    {% endfor %}
  </table>
{% endblock %}

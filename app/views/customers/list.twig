{% extends "layout.twig" %}

{% block content %}
  <div class="page-header">
    <h3>Customers</h3>
  </div>
  <table class="table">
    <thead>
      <th>#</th>
      <th>Editor #</th>
    </thead>
    {% for customer in customers %}
    <tr>
      <td><a href="/admin/customers/{{customer.id}}/">{{customer.id}}</a></td>
      <td>{{customer.editor_id}}</td>
    </tr>
    {% endfor %}
  </table>
{% endblock %}
